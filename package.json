    "url": "https://github.com/fernancumez/restapi-jwt-typescript/issues"
  },
  "homepage": "https://github.com/fernancumez/restapi-jwt-typescript#readme",
  "dependencies": {
    "bcrypt": "5.0.0",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.11.18",
    "morgan": "1.10.0",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0"
  },
  "devDependencies": {
