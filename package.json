{
  "name": "jwt-restapi",
  "version": "0.0.1",
  "description": "Ejemplo práctico  de Javascript/Typescript donde se hará la creación de una REST API usando Nodejs, Mongodb (a traves de Mongoose) adicionalmente de modulos como bcrypt y passport.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "start": "export NODE_ENV=production || SET NODE_ENV=production&& npm run build && node dist/index.js",
    "dev": "export NODE_ENV=production || SET NODE_ENV=development&& nodemon dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fernancumez/restapi-jwt-typescript.git"
  },
  "keywords": [],
