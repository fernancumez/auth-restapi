    "@types/mongoose": "5.10.3",
    "@types/morgan": "1.9.0",
    "@types/passport": "1.0.3",
    "@types/passport-jwt": "3.0.3",
    "nodemon": "2.0.7",
    "tsc-watch": "4.2.3",
    "typescript": "4.2.2"
